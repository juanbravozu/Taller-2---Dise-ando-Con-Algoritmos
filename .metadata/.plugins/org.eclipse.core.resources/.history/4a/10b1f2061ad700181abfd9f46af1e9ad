package mquevedojbravo;

import processing.core.PApplet;

public class Main extends PApplet {
	
	Logica log;
	
	public static void main(String[] args) {
		PApplet.main("mquevedojbravo.Main");
	}
	
	public void settings() {
		size(1200, 676);
	}
	
	public void setup() {
		log = new Logica(this);
		imageMode(CENTER);
		noStroke();
		noCursor();
	}
	
	public void draw() {
		log.pintar();
	}
	
	public void keyPressed() {
		
	}
}
